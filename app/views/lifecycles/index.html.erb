<h1>All lifecycles</h1>

<table>
<% @lifecycles.each do |lifecycle| %>
  <tr>
    <td><%= link_to lifecycle.name, lifecycle %></td>
    <td><%= lifecycle.description %></td>
  </tr>
<% end %>
</table>

<br />
<%= button_to "Cancel", {:controller => "lifecycles", :action => "home"}, :class => "btn" %>
